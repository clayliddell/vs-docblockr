{
  "name": "docblock-generator",
  "displayName": "DocBlock Generator",
  "description": "Manages doc blocks with visual studio code",
  "version": "0.0.1",
  "publisher": "jeremyljackson",
  "engines": {
    "vscode": "^1.14.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.init",
        "title": "DocBlock Generator - Parse"
      }
    ]
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.1.4",
    "xregexp": "^3.2.0"
  },
  "dependencies": {
    "pug-lexer": "^3.1.0",
    "tsc": "^1.20150623.0"
  }
}
