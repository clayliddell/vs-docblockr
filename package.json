{
  "name": "docblock-generator",
  "displayName": "DocBlock Generator",
  "description": "Manages doc blocks with visual studio code",
  "version": "0.0.1",
  "publisher": "jeremyljackson",
  "engines": {
    "vscode": "^1.14.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "contributes": {
    "commands": [{
      "command": "extension.parseFunction",
      "title": "DocBlock Generator - Parse Function"
    }]
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  }
}